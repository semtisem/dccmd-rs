    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'dccmd-rs'",
            "cargo": {
                "args": [
                    "build",
                    "--bin=dccmd-rs",
                    "--package=dccmd-rs"
                ],
                "filter": {
                    "name": "dccmd-rs",
                    "kind": "bin"
                }
            },
            "args": ["--debug", "upload", "-r", "/home/oc/dev/dccmd-rs/src", "demo-os.dracoon.com/DEMO_UBPF"],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug unit tests in executable 'dccmd-rs'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--bin=dccmd-rs",
                    "--package=dccmd-rs"
                ],
                "filter": {
                    "name": "dccmd-rs",
                    "kind": "bin"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        }
    ]
}